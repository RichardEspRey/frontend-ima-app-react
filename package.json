{
  "name": "imaappdesktopreact",
  "private": true,
  "version": "1.0.6",
  "type": "module",
  "main": "electron.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "docs:dev": "npx htmldocs@latest dev",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "electron:build": "npm run dist"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.3.4",
    "@mui/x-charts": "^8.14.1",
    "@reduxjs/toolkit": "^2.6.1",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "electron-updater": "^6.6.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "leaflet": "^1.9.4",
    "react": "^19.0.0",
    "react-datepicker": "^8.2.1",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-photo-view": "^1.2.7",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.4.1",
    "react-select": "^5.10.1",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.28.0",
    "sweetalert2": "^11.17.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  },
  "build": {
    "appId": "com.imaexpress.desktopapp",
    "productName": "IMA Desktop App",
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron.cjs",
      "preload.js"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": ["dmg", "zip"],
      "category": "public.app-category.utilities"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "RichardEspRey",
        "repo": "frontend-ima-app-react-mac",
        "private": true
      }
    ]
  }
}
